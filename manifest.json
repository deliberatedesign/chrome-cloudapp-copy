{
  "manifest_version": 2,
  "name": "NUX CloudApp Copy",
  "version": "0.0.3",
  "minimum_chrome_version": "70.0",
  "description": "Copy CloudApp image source",
  "author": "Nitro UX",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "NUX CloudApp Copy"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
